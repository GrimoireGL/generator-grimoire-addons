{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= desc %>",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "./lib",
    "./samples"
  ],
  "keywords": [
    "grimoire.js"
  ],
  "dependencies": {},
  "devDependencies": {},
  "repository": "<%= repo %>",
  "scripts": {
    "scafold": "babel-node ./build/scafold.js --presets es2015,stage-2 --plugins transform-runtime",
    "minify": "uglify -s ./product/index.js -o ./product/index.min.js",
    "compile": "tsc @./lib-ts/entry_files --declaration --outDir ./lib -m es6 -t es6 --moduleResolution node --sourcemap",
    "build": "babel-node rollup.config.js --presets es2015,stage-2 --plugins transform-runtime",
    "postinstall": "typings install",
    "babel": "babel ./product/index.es2016.js --out-file ./product/index.js --inputSourceMap ./product/index.es2016.js.map --sourceMaps true -sourceMapTarget ./index.js.map",
    "prepublish": "babel-node ./build/prepublish.js --presets es2015,stage-2 --plugins transform-runtime",
    "start": "npm run build -- -w -s",
    "serve": "hs ./",
    "babel-test": "babel ./lib  --out-dir ./lib-es5",
    "ava": "ava --verbose",
    "test": "babel-node ./build/test-runner.js --presets es2015,stage-2 --plugins transform-runtime"
  },
  "grimoire": {
    "namespace": "HTTP://GRIMOIRE.GL/NS/CUSTOM",
    "dependencies": []
  },
  "license": "MIT"
}
