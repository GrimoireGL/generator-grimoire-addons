{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= desc %>",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "./lib",
    "./samples"
  ],
  "keywords": [
    "grimoire.js"
  ],
  "dependencies": {},
  "devDependencies": {},
  "repository": "<%= repo %>",
  "scripts": {
    "scafold": "babel-node ./build/scafold.js --presets es2015,stage-2 --plugins transform-runtime",
    "compile": "tsc @./lib-ts/entry_files --declaration --outDir ./lib -m es6 -t es6 --moduleResolution node ",
    "build": "babel-node rollup.config.js --presets es2015,stage-2 --plugins transform-runtime",
    "postinstall": "typings install",
    "bundle": "babel ./product/index.es2016.js --out-file ./product/index.js",
    "prepublish": "babel-node ./build/prepublish.js --presets es2015,stage-2 --plugins transform-runtime",
    "start": "npm run build & npm run serve",
    "serve": "hs ./"
  },
  "grimoire": {
    "namespace": "HTTP://GRIMOIRE.GL/NS/CUSTOM",
    "dependencies": []
  },
  "license": "MIT"
}
